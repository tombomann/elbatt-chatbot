WORKDIR /job
COPY requirements.txt .
RUN pip install --no-cache-dir -r requirements.txt
COPY job_runner.py .
# Default kommando: les PLATE fra env og gjÃ¸r Playwright-scrape -> skriv til stdout/lagre
